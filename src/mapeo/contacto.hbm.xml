<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Contacto" table="contacto">
		<id column="idContacto" name="idContacto">
			<generator class="assigned"/>
		</id>
		<property column="email"  name="email" type="string"></property>
		<property column="movil"  name="movil" type="string"></property>
		<property column="fijo"  name="fijo" type="string"></property>
		 
	</class>
</hibernate-mapping>